{
	"name": "ejs",
	"description": "Syntax Highlighting for JavaScript EJS files",
	"version": "1.0.0",
	"publisher": "ezekg",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Languages"
	],
	"contributes": {
		"languages": [{
			"id": "ejs",
			"aliases": ["JavaScript EJS", "ejs"],
			"extensions": [".ejs",".rhtml",".html.ejs"],
			"configuration": "./ejs.configuration.json"
		}],
		"grammars": [{
			"language": "ejs",
			"scopeName": "text.html.ejs",
			"path": "./syntaxes/ejs.tmLanguage"
		}]
	},
	"repository": {
        "type": "git",
        "url": "https://github.com/craigmaslowski/vscode-ejs.git"
    }
}